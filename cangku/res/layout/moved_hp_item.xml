<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/dh"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="R2013090955555"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/date"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="20130909"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/op"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="入库"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/type"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="2"
        android:text="类型"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/ck"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="仓库"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/num"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="5555555"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/dj"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="5555555"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/zj"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="5555.555"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />
    <TextView
        android:id="@+id/depName"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="5555555"
        android:textColor="@color/black" />
	<View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />
    <TextView
        android:id="@+id/dw"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="5555555"
        android:textColor="@color/black" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/black" />

    <TextView
        android:id="@+id/jbr"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_margin="2dp"
        android:gravity="center"
        android:maxLines="2"
        android:text="55555"
        android:textColor="@color/black" />

</LinearLayout>